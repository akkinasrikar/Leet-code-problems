    "class Solution:\n",
    "    def climbStairs(self, n):\n",
    "        if n==1: return 1\n",
    "        if n==2: return 2\n",
    "        l=[0]*(n+2)\n",
    "        l[1],l[2]=1,2\n",
    "        for i in range(2,n+2):\n",
    "            l[i]=l[i-1]+l[i-2]\n",
    "        #print(l)\n",
    "        return l[-1]\n",
    "        "
   ]
  }
