    "#(2nCn)/(n+1)\n",
    "import math\n",
    "class Solution:\n",
    "    def numTrees(self,n):\n",
    "        return int((math.factorial(2*n)/(math.factorial(n)*math.factorial(2*n-n)))/(n+1))\n",
    "        "
   ]
  },
